    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="prebuild, accesories, monitor, keyboard, mouse, membership" type="String" array="False" size=""/>
            <declare name="casePrice, motherboardPrice, RAMPrice, cpuPrice, gpuPrice, storagePrice, PSUPrice, accesoriesPrice, prebuildPrice, membershipDiscount, totalPrice, priceAfterDiscount, priceAfterTax" type="Real" array="False" size=""/>
            <assign variable="casePrice" expression="0"/>
            <assign variable="motherboardPrice" expression="0"/>
            <assign variable="RAMPrice" expression="0"/>
            <assign variable="prebuildPrice" expression="0"/>
            <assign variable="cpuPrice" expression="0"/>
            <assign variable="gpuPrice" expression="0"/>
            <assign variable="storagePrice" expression="0"/>
            <assign variable="PSUPrice" expression="0"/>
            <output expression="&quot;Welcome to our PC building system&quot;" newline="True"/>
            <do expression="prebuild != &quot;Y&quot; &amp;&amp; prebuild != &quot;y&quot; &amp;&amp; prebuild != &quot;N&quot; &amp;&amp; prebuild != &quot;n&quot;">
                <output expression="&quot;Do you want a prebuild?(Y/N):&quot;" newline="True"/>
                <input variable="prebuild"/>
                <if expression="prebuild == &quot;Y&quot; || prebuild == &quot;y&quot;">
                    <then>
                        <assign variable="prebuildPrice" expression="prebuild()"/>
                    </then>
                    <else>
                        <if expression="prebuild == &quot;N&quot; || prebuild == &quot;n&quot;">
                            <then>
                                <output expression="&quot;For the first PC part, please choose your case&quot;" newline="True"/>
                                <assign variable="casePrice" expression="case()"/>
                                <assign variable="motherboardPrice" expression="motherboard()"/>
                                <assign variable="RAMPrice" expression="ram()"/>
                                <assign variable="cpuPrice" expression="cpu()"/>
                                <assign variable="gpuPrice" expression="gpu()"/>
                                <assign variable="storagePrice" expression="storage()"/>
                                <assign variable="PSUPrice" expression="psu()"/>
                            </then>
                            <else>
                                <output expression="&quot;Wrong Input, please try again!&quot;" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
            </do>
            <assign variable="accesoriesPrice" expression="accesories()"/>
            <do expression="membership != &quot;Y&quot; &amp;&amp; membership != &quot;y&quot; &amp;&amp; membership != &quot;N&quot; &amp;&amp; membership != &quot;n&quot;">
                <output expression="&quot;Do you have a membership?(Y/N)&quot;" newline="True"/>
                <input variable="membership"/>
                <if expression="membership == &quot;Y&quot; || membership == &quot;y&quot;">
                    <then>
                        <assign variable="membershipDiscount" expression="20"/>
                    </then>
                    <else>
                        <if expression="membership == &quot;N&quot; || membership == &quot;n&quot;">
                            <then>
                                <assign variable="membershipDiscount" expression="0"/>
                            </then>
                            <else>
                                <output expression="&quot;Wrong Input, please try again!&quot;" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
            </do>
            <assign variable="totalPrice" expression="Calctotalprice(casePrice, motherboardPrice, RAMPrice, cpuPrice, gpuPrice, storagePrice, PSUPrice, prebuildPrice, accesoriesPrice)"/>
            <assign variable="priceAfterDiscount" expression="Calcpricediscount(totalprice,membershipDiscount)"/>
            <assign variable="priceAfterTax" expression="Calcpricetax(priceAfterDiscount)"/>
            <call expression="Display(totalPrice, membershipDiscount, priceAfterDiscount, priceAfterTax, casePrice, motherboardPrice, RAMPrice, cpuPrice, gpuPrice, storagePrice, PSUPrice, accesoriesPrice, prebuildPrice)"/>
        </body>
    </function>
    <function name="accesories" type="Real" variable="accesoriesPrice">
        <parameters/>
        <body>
            <declare name="accesoriesPrice, monitorPrice, keyboardPrice, mousePrice" type="Real" array="False" size=""/>
            <declare name="accesories, monitor, keyboard, mouse" type="String" array="False" size=""/>
            <do expression="accesories != &quot;Y&quot; &amp;&amp; accesories != &quot;y&quot; &amp;&amp; accesories != &quot;N&quot; &amp;&amp; accesories != &quot;n&quot;">
                <output expression="&quot;Do you want extra accesories with your PC?(Y/N)&quot;" newline="True"/>
                <input variable="accesories"/>
                <if expression="accesories == &quot;Y&quot; || accesories == &quot;y&quot;">
                    <then>
                        <do expression="monitor != &quot;y&quot; &amp;&amp; monitor != &quot;Y&quot; &amp;&amp; monitor != &quot;n&quot; &amp;&amp; monitor != &quot;N&quot;">
                            <output expression="&quot;Would you like to add-on a Monitor?(Y/N)&quot;" newline="True"/>
                            <input variable="monitor"/>
                            <if expression="monitor == &quot;y&quot; || monitor == &quot;Y&quot;">
                                <then>
                                    <output expression="&quot;We recommend the Acer K202HQL LED Monitor With HD MI (20 inch) for RM120&quot;" newline="True"/>
                                    <assign variable="monitorPrice" expression="120"/>
                                </then>
                                <else>
                                    <if expression="monitor == &quot;n&quot; || monitor == &quot;N&quot;">
                                        <then>
                                            <assign variable="monitorPrice" expression="0"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;Wrong Input, please try again!&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </do>
                        <do expression="keyboard != &quot;Y&quot; &amp;&amp; keyboard != &quot;y&quot; &amp;&amp; keyboard != &quot;N&quot; &amp;&amp; keyboard != &quot;n&quot;">
                            <output expression="&quot;Would you like to add-on a keyboard?(Y/N)&quot;" newline="True"/>
                            <input variable="keyboard"/>
                            <if expression="keyboard == &quot;Y&quot; || keyboard == &quot;y&quot;">
                                <then>
                                    <output expression="&quot;We recommend the Logitech G213 Prodigy Wired USB Gaming Keyboard for RM80&quot;" newline="True"/>
                                    <assign variable="keyboardPrice" expression="80"/>
                                </then>
                                <else>
                                    <if expression="keyboard == &quot;N&quot; || keyboard == &quot;n&quot;">
                                        <then>
                                            <assign variable="keyboardPrice" expression="0"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;Wrong Input, please try again!&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </do>
                        <do expression="mouse != &quot;Y&quot; &amp;&amp; mouse != &quot;y&quot; &amp;&amp; mouse != &quot;N&quot; &amp;&amp; mouse != &quot;n&quot;">
                            <output expression="&quot;Would you like to add-on a mouse?(Y/N)&quot;" newline="True"/>
                            <input variable="mouse"/>
                            <if expression="mouse == &quot;Y&quot; || mouse == &quot;y&quot;">
                                <then>
                                    <output expression="&quot;We recommend the Logitech G102 Lightsync Wired USB Gaming Mouse for RM50&quot;" newline="True"/>
                                    <assign variable="mousePrice" expression="50"/>
                                </then>
                                <else>
                                    <if expression="mouse == &quot;N&quot; || mouse == &quot;n&quot;">
                                        <then>
                                            <assign variable="mousePrice" expression="0"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;Wrong Input, please try again!&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </do>
                    </then>
                    <else>
                        <if expression="accesories == &quot;N&quot; || accesories == &quot;n&quot;">
                            <then>
                                <assign variable="monitorPrice" expression="0"/>
                                <assign variable="keyboardPrice" expression="0"/>
                                <assign variable="mousePrice" expression="0"/>
                            </then>
                            <else>
                                <output expression="&quot;Wrong Input, please try again!&quot;" newline="True"/>
                            </else>
                        </if>
                    </else>
                </if>
            </do>
            <assign variable="accesoriesPrice" expression="monitorPrice + keyboardPrice + mousePrice"/>
        </body>
    </function>
    <function name="Calcpricediscount" type="Real" variable="priceafterdiscount">
        <parameters>
            <parameter name="totalprice" type="Real" array="False"/>
            <parameter name="membershipDiscount" type="Real" array="False"/>
        </parameters>
        <body>
            <declare name="priceafterdiscount" type="Real" array="False" size=""/>
            <assign variable="priceafterdiscount" expression="totalprice*((100-membershipDiscount)/100)"/>
        </body>
    </function>
    <function name="Calcpricetax" type="Real" variable="pricetax">
        <parameters>
            <parameter name="priceAfterDiscount" type="Real" array="False"/>
        </parameters>
        <body>
            <declare name="pricetax" type="Real" array="False" size=""/>
            <assign variable="pricetax" expression="priceAfterDiscount* 106/100"/>
        </body>
    </function>
    <function name="Calctotalprice" type="Real" variable="totalprice">
        <parameters>
            <parameter name="casePrice" type="Real" array="False"/>
            <parameter name="motherboardPrice" type="Real" array="False"/>
            <parameter name="RAMPrice" type="Real" array="False"/>
            <parameter name="cpuPrice" type="Real" array="False"/>
            <parameter name="gpuPrice" type="Real" array="False"/>
            <parameter name="storagePrice" type="Real" array="False"/>
            <parameter name="PSUPrice" type="Real" array="False"/>
            <parameter name="prebuildPrice" type="Real" array="False"/>
            <parameter name="accesoriesPrice" type="Real" array="False"/>
        </parameters>
        <body>
            <declare name="totalprice" type="Real" array="False" size=""/>
            <assign variable="totalprice" expression="casePrice + motherboardPrice + RAMPrice + cpuPrice + gpuPrice + storagePrice + PSUPrice + prebuildPrice + accesoriesPrice"/>
        </body>
    </function>
    <function name="case" type="Real" variable="casePrice">
        <parameters/>
        <body>
            <declare name="casePrice" type="Real" array="False" size=""/>
            <declare name="case" type="String" array="False" size=""/>
            <do expression="case != &quot;A&quot; &amp;&amp; case != &quot;a&quot; &amp;&amp; case != &quot;B&quot; &amp;&amp; case != &quot;b&quot; &amp;&amp; case != &quot;C&quot; &amp;&amp; case != &quot;c&quot;">
                <output expression="&quot;You only have to input A, B or C to choose your PC part&quot;" newline="True"/>
                <output expression="&quot;Would you like: A. big case B. medium case C. small case&quot;" newline="True"/>
                <input variable="case"/>
                <if expression="case == &quot;A&quot; || case == &quot;a&quot;">
                    <then>
                        <assign variable="casePrice" expression="150"/>
                    </then>
                    <else>
                        <if expression="case == &quot;B&quot; || case == &quot;b&quot;">
                            <then>
                                <assign variable="casePrice" expression="100"/>
                            </then>
                            <else>
                                <if expression="case == &quot;C&quot; || case == &quot;c&quot;">
                                    <then>
                                        <assign variable="casePrice" expression="50"/>
                                    </then>
                                    <else/>
                                </if>
                            </else>
                        </if>
                    </else>
                </if>
            </do>
        </body>
    </function>
