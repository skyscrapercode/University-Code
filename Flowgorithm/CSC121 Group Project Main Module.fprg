    <function name="Display" type="None" variable="">
        <parameters>
            <parameter name="totalPrice" type="Real" array="False"/>
            <parameter name="membershipDiscount" type="Real" array="False"/>
            <parameter name="priceAfterDiscount" type="Real" array="False"/>
            <parameter name="priceAfterTax" type="Real" array="False"/>
            <parameter name="casePrice" type="Real" array="False"/>
            <parameter name="motherboardPrice" type="Real" array="False"/>
            <parameter name="RAMPrice" type="Real" array="False"/>
            <parameter name="cpuPrice" type="Real" array="False"/>
            <parameter name="gpuPrice" type="Real" array="False"/>
            <parameter name="storagePrice" type="Real" array="False"/>
            <parameter name="PSUPrice" type="Real" array="False"/>
            <parameter name="accesoriesPrice" type="Real" array="False"/>
            <parameter name="prebuildPrice" type="Real" array="False"/>
        </parameters>
        <body>
            <output expression="&quot;*****ORDER RECEIPT*****&quot;" newline="True"/>
            <if expression="casePrice &gt; 0">
                <then>
                    <output expression="&quot;Case = RM &quot;&amp;casePrice" newline="True"/>
                    <output expression="&quot;Motherboard = RM &quot;&amp;motherboardPrice" newline="True"/>
                    <output expression="&quot;RAM = RM &quot;&amp;RAMPrice" newline="True"/>
                    <output expression="&quot;CPU = RM &quot;&amp;cpuPrice" newline="True"/>
                    <output expression="&quot;GPU = RM &quot;&amp;gpuPrice" newline="True"/>
                    <output expression="&quot;Storage = RM &quot;&amp;storagePrice" newline="True"/>
                    <output expression="&quot;PSU = RM &quot;&amp;PSUPrice" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;PREBUILD = RM &quot; &amp;prebuildPrice" newline="True"/>
                </else>
            </if>
            <if expression="accesoriesPrice &gt; 0">
                <then>
                    <output expression="&quot;Accessories = RM &quot;&amp; accesoriesPrice" newline="True"/>
                </then>
                <else/>
            </if>
            <output expression="&quot;The Total Price is RM&quot; &amp;totalPrice" newline="True"/>
            <output expression="&quot;The price after &quot; &amp;membershipDiscount&amp;&quot;% discount is RM&quot; &amp;priceAfterDiscount" newline="True"/>
            <output expression="&quot;The price after 6% tax is RM&quot; &amp;priceAfterTax" newline="True"/>
            <output expression="&quot;Thank you for using our PC Builder!&quot;" newline="True"/>
        </body>
    </function>
