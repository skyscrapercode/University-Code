    <function name="prebuild" type="Real" variable="prebuildPrice">
        <parameters/>
        <body>
            <declare name="prebuildPrice" type="Real" array="False" size=""/>
            <declare name="prebuild" type="Integer" array="False" size=""/>
            <output expression="&quot;We can recommend top quality prebuilds such as HP, ACER, ASUS, MSI&quot;" newline="True"/>
            <do expression="prebuild != 1 &amp;&amp; prebuild != 2 &amp;&amp; prebuild != 3 &amp;&amp; prebuild != 4">
                <output expression="&quot;You only have to input 1, 2, 3 or 4 to choose your prebuild&quot;" newline="True"/>
                <output expression="&quot;Which brand would you like to choose? 1. HP 2. ACER 3. ASUS 4. MSI&quot;" newline="True"/>
                <input variable="prebuild"/>
                <if expression="prebuild == 4">
                    <then>
                        <output expression="&quot;MSI: We recommend the MSI PRO 5S GAMING PC for RM4199&quot;" newline="True"/>
                        <assign variable="prebuildPrice" expression="4199"/>
                    </then>
                    <else>
                        <if expression="prebuild == 3">
